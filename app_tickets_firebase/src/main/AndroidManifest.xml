<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.study.app_tickets_firebase">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ticket"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ticket"
        android:supportsRtl="true"
        android:theme="@style/Theme.ArduinoApp">
        <activity android:name=".ConsoleActivity"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".OrderListActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>